	// if P = ∞  and Q <> ∞ then P <> Q
	[
		"0",
		"0",
		"d74bf844b0862475103d96a611cf2d898447e288d34b360bc885cb8ce7c00575",
		"131c670d414c4546b88ac3ff664611b1c38ceb1c21d76369d7a7a0969d61d97d",
